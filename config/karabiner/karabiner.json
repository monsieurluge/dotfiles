            "virtual_hid_keyboard": {
                "country_code": 0,
                "indicate_sticky_modifier_keys_state": true,
                "mouse_key_xy_scale": 100
            },
            "simple_modifications": [
                {
                    "from": { "key_code": "backslash" },
                    "to": [{ "key_code": "delete_or_backspace" }]
                },
                {
                    "from": { "key_code": "tab" },
                    "to": [{ "key_code": "escape" }]
                },
                {
                    "from": { "key_code": "caps_lock" },
                    "to": [{ "key_code": "tab" }]
                },
                {
